<!--DOCTYPE html -->
<html>
<head>
  <% include ../partials/head %>
</head>
<body>

<div id="page" class="page">
  <header id="header-1">
    <% include ../partials/header %>
  </header>
</div>
  <section id="admin-skus" class="">
    <div class="container">
      <!--<h2>Admin Skus</h2>-->
      <div class="row">
        <form action="/skus-create" method="post" id="skus-create">
          <div class="">
            <button type="button"  class="btn btn-primary btn-sm pull-right" data-toggle="modal" data-target="#add-modal">
              <b>New SKUS</b>
            </button>
          </div>

          <table id="admin-skus-tbl" class="display">
            <thead>
            <tr>
              <th>Act</th>
              <th>ID</th>
              <th>Manufacturer</th>
              <th>model</th>
              <th>Type</th>
              <th>Category</th>
              <th>Total Quantity</th>
              <th>Quantity Available</th>
              <th>Intro Date</th>
              <th>Market Price</th>
              <th>Links</th>
              <th>Related Skus</th>
              <th>Text</th>
              <th>Description</th>
              <th>Reviews</th>
            </tr>
            </thead>
            <tbody id="skus-tbl-body">
              <% skus.map(function(sku) { %>
                <tr>
                  <td><a data-id="row-<%= sku.id%>" href="javascript:editRow('<%= sku.id%>');">U</a> / D</td>
                  <td><%= sku.id%></td>
                  <td><%= sku.manufacturer%></td>
                  <td><%= sku.model%></td>
                  <td><%= sku.type%></td>
                  <td><%= sku.category%></td>
                  <td><%= sku.totalquantity%></td>
                  <td><%= sku.quantityavailable%></td>
                  <td><%= new Date(sku.introdate).toLocaleDateString()%></td>
                  <td><%= sku.marketprice%></td>
                  <td><%= sku.links%></td>
                  <td><%= sku.relatedskus%></td>
                  <td><%= sku.text%></td>
                  <td><%= sku.description%></td>
                  <td><%= sku.reviews%></td>
                </tr>
              <% }) %>
            </tbody>
          </table>
        </form>
      </div>
    </div>


    <!-- Add Modal-->
    <div class="modal fade" id="add-modal" tabindex="-1" role="dialog" aria-labelledby="add-modal-label">
      <div class="modal-dialog" role="document">
        <div class="modal-content">
          <form class="form-horizontal" id="add-form">
            <div class="modal-header">
              <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
              <h4 class="modal-title" id="add-modal-label">Add New SKUS</h4>
            </div>
            <div class="modal-body">
              <div class="form-group">
                <label for="skus-id" class="col-sm-2 control-label">*ID:</label>
                <div class="col-sm-10">
                  <input type="text" id="skus-id" class="form-control" placeholder="*ID" required>
                </div>
              </div>
              <div class="form-group">
                <label for="skus-manufacturer" class="col-sm-2 control-label">*Manufacturer:</label>
                <div class="col-sm-10">
                  <input type="text" id="skus-manufacturer" class="form-control" placeholder="*Manufacturer" required>
                </div>
              </div>
              <div class="form-group">
                <label for="skus-model" class="col-sm-2 control-label">*Model:</label>
                <div class="col-sm-10">
                  <input type="text" id="skus-model" class="form-control" placeholder="*Model" required>
                </div>
              </div>
              <div class="form-group">
                <label for="skus-type" class="col-sm-2 control-label">*Type:</label>
                <div class="col-sm-10">
                  <input type="text" id="skus-type" class="form-control" placeholder="*Type" required>
                </div>
              </div>
              <div class="form-group">
                <label for="skus-category" class="col-sm-2 control-label">Category:</label>
                <div class="col-sm-10">
                  <input type="text" id="skus-category" class="form-control" placeholder="Category">
                </div>
              </div>
              <div class="form-group">
                <label for="skus-totalquantity" class="col-sm-2 control-label">*TotQuant:</label>
                <div class="col-sm-10">
                  <input type="number" id="skus-totalquantity" class="form-control" placeholder="*Total Quantity" required>
                </div>
              </div>
              <div class="form-group">
                <label for="skus-quantityavailable" class="col-sm-2 control-label">*QuanAvl:</label>
                <div class="col-sm-10">
                  <input type="number" id="skus-quantityavailable" class="form-control" placeholder="*Quantity Available" required>
                </div>
              </div>
              <div class="form-group">
                <label for="skus-marketprice" class="col-sm-2 control-label">*Market Price</label>
                <div class="col-sm-10">
                  <input type="number" id="skus-marketprice" class="form-control" placeholder="*marketprice" required>
                </div>
              </div>
              <div class="form-group">
                <label for="skus-links" class="col-sm-2 control-label">Links</label>
                <div class="col-sm-10">
                  <input type="text" id="skus-links" class="form-control" placeholder="links">
                </div>
              </div>
              <div class="form-group">
                <label for="skus-relatedskus" class="col-sm-2 control-label">Related Skus</label>
                <div class="col-sm-10">
                  <input type="text" id="skus-relatedskus" class="form-control" placeholder="relatedskus">
                </div>
              </div>
              <div class="form-group">
                <label for="skus-introdate" class="col-sm-2 control-label">*Intro Date</label>
                <div class="col-sm-10">
                  <input type="text" id="skus-introdate" class="form-control" placeholder="*introdate" required>
                </div>
              </div>
              <div class="form-group">
                <label for="skus-description" class="col-sm-2 control-label">Description</label>
                <div class="col-sm-10">
                  <input type="text" id="skus-description" class="form-control" placeholder="description">
                </div>
              </div>
              <div class="form-group">
                <label for="skus-reviews" class="col-sm-2 control-label">Reviews</label>
                <div class="col-sm-10">
                  <input type="text" id="skus-reviews" class="form-control" placeholder="reviews">
                </div>
              </div>
              <div class="form-group">
                <label for="skus-text" class="col-sm-2 control-label">*Text</label>
                <div class="col-sm-10">
                  <textarea id="skus-text" class="form-control" placeholder="*text" required rows="1" cols="80"></textarea>
                </div>
              </div>
            </div>
            <div class="modal-footer">
              <button type="button" class="btn btn-default nomargin" data-dismiss="modal">Close</button>
              <button type="submit" class="btn btn-primary nomargin">Insert</button>
            </div>
          </form>
        </div>
      </div>
    </div>

    <!-- Edit Modal-->
    <div class="modal fade" id="edit-modal" tabindex="-1" role="dialog" aria-labelledby="edit-modal-label">
      <div class="modal-dialog" role="document">
        <div class="modal-content">
          <form class="form-horizontal" id="edit-form">
            <div class="modal-header">
              <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
              <h4 class="modal-title" id="edit-modal-label">Update SKUS Data</h4>
            </div>
            <div class="modal-body">
                <div class="form-group">
                <label for="edit-skus-id" class="col-sm-2 control-label">*ID:</label>
                <div class="col-sm-10">
                  <input type="text" id="edit-skus-id" class="form-control" name="skus-id" placeholder="*ID" readonly>
                </div>
              </div>
              <div class="form-group">
                <label for="edit-skus-manufacturer" class="col-sm-2 control-label">*Manufacturer:</label>
                <div class="col-sm-10">
                  <input type="text" id="edit-skus-manufacturer" class="form-control" placeholder="*Manufacturer" required>
                </div>
              </div>
              <div class="form-group">
                <label for="edit-skus-model" class="col-sm-2 control-label">*Model:</label>
                <div class="col-sm-10">
                  <input type="text" id="edit-skus-model" class="form-control" placeholder="*Model" required>
                </div>
              </div>
              <div class="form-group">
                <label for="edit-skus-type" class="col-sm-2 control-label">*Type:</label>
                <div class="col-sm-10">
                  <input type="text" id="edit-skus-type" class="form-control" placeholder="*Type" required>
                </div>
              </div>
              <div class="form-group">
                <label for="edit-skus-category" class="col-sm-2 control-label">Category:</label>
                <div class="col-sm-10">
                  <input type="text" id="edit-skus-category" class="form-control" placeholder="Category">
                </div>
              </div>
              <div class="form-group">
                <label for="edit-skus-totalquantity" class="col-sm-2 control-label">*TotQuant:</label>
                <div class="col-sm-10">
                  <input type="number" id="edit-skus-totalquantity" class="form-control" placeholder="*Total Quantity" required>
                </div>
              </div>
              <div class="form-group">
                <label for="edit-skus-quantityavailable" class="col-sm-2 control-label">*QuanAvl:</label>
                <div class="col-sm-10">
                  <input type="number" id="edit-skus-quantityavailable" class="form-control" placeholder="*Quantity Available" required>
                </div>
              </div>
              <div class="form-group">
                <label for="edit-skus-marketprice" class="col-sm-2 control-label">*Market Price</label>
                <div class="col-sm-10">
                  <input type="number" id="edit-skus-marketprice" class="form-control" placeholder="*marketprice" required>
                </div>
              </div>
              <div class="form-group">
                <label for="edit-skus-links" class="col-sm-2 control-label">Links</label>
                <div class="col-sm-10">
                  <input type="text" id="edit-skus-links" class="form-control" placeholder="links">
                </div>
              </div>
              <div class="form-group">
                <label for="edit-skus-relatedskus" class="col-sm-2 control-label">Related Skus</label>
                <div class="col-sm-10">
                  <input type="text" id="edit-skus-relatedskus" class="form-control" placeholder="relatedskus">
                </div>
              </div>
              <div class="form-group">
                <label for="edit-skus-introdate" class="col-sm-2 control-label">*Intro Date</label>
                <div class="col-sm-10">
                  <input type="text" id="edit-skus-introdate" class="form-control" placeholder="*introdate" required>
                </div>
              </div>
              <div class="form-group">
                <label for="edit-skus-description" class="col-sm-2 control-label">Description</label>
                <div class="col-sm-10">
                  <input type="text" id="edit-skus-description" class="form-control" placeholder="description">
                </div>
              </div>
              <div class="form-group">
                <label for="edit-skus-reviews" class="col-sm-2 control-label">Reviews</label>
                <div class="col-sm-10">
                  <input type="text" id="edit-skus-reviews" class="form-control" placeholder="reviews">
                </div>
              </div>
              <div class="form-group">
                <label for="edit-skus-text" class="col-sm-2 control-label">*Text</label>
                <div class="col-sm-10">
                  <textarea id="edit-skus-text" class="form-control" placeholder="*text" required rows="1" cols="80"></textarea>
                </div>
              </div>
            </div>
            <div class="modal-footer">
              <button type="button" class="btn btn-default nomargin" data-dismiss="modal">Close</button>
              <button type="submit" class="btn btn-primary nomargin">Update</button>
            </div>
            </form>
        </div>
      </div>
    </div>
  </section>

  <%include ../partials/footer %>

<!-- Theme Scripts (Do not remove) -->

<% include ../partials/scripts %>
<% include ../partials/admin-scripts %>
</body>
</html>
